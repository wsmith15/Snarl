package info.gridworld.actor;
import java.awt.Color;

public class ImmuneWall extends Actor
{
    
    
    private static final Color DEFAULT_COLOR = Color.BLACK;

    public ImmuneWall()
    {
        setColor(DEFAULT_COLOR);
    }


    public ImmuneWall(Color thisColor)
    {
        setColor(thisColor);
    }
    public void act()
    {
    }
}

